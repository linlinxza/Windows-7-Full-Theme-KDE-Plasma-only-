style "notebook"	= "default"
{
 xthickness = 2
 ythickness = 2

 engine "pixmap"
  {

	image
	{
		function	= EXTENSION
		state		= NORMAL
		gap_side	= BOTTOM
		file		= "/Tabs/tab-top-active.png"
		stretch		= TRUE
		border		= { 5 ,5 ,5 ,5}
	}

	image
	{
		function	= EXTENSION
		state		= ACTIVE
		gap_side	= BOTTOM
		file		= "/Tabs/tab-top.png"
		stretch		= TRUE
		border		= { 5 ,5 ,5 ,5}
	}


	image
	{
		function	= EXTENSION
		state		= NORMAL
		gap_side	= TOP
