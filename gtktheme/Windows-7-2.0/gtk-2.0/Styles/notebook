		gap_start_border= { 1 ,1 ,1 ,1}
		gap_end_file	= "/Tabs/null.png"
		gap_end_border	= { 1 ,1 ,1 ,1}
		
	}

	image
	{
		function	= BOX_GAP
		gap_side	= BOTTOM
		file		= "/Tabs/notebook_bottom_flat.png"
		stretch		= TRUE
		border		= { 5 ,3 ,3 ,3}
		gap_file	= "/Tabs/tab-bottom-active-gap.png"
		gap_border	= { 3 ,3 ,1 ,1}
